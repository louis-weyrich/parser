{
	"bufferSize":"256",
	"autoMarkBuffer":"true",
	"initialBufferSize":"20",
	"bufferGrowthSize":"20",
	"tokens":[" ", "&", "|", "{", "}", "(", ")", "[","]","\"", "'", ",", ":", "\n", "\r", "\t", "@", ";", "="],
	"tokenSets": ["//","/*","/**","*/","/#","#/"],
	"tokensNotAllowed":["`", "~"],
	"ignorableTokens":[" ","\n","\r","\t"],
	"showIgnorable":"false",
	"statementEnd":[";"],
	"matchedTokens":
	[
		{"match":["[","]"]},
		{"match":["{","}"]},
		{"match":["(",")"]}
	],
	"matchQuotes":"true",
	"keywords": ["select","from","where","in","ignorableTokens","tokens","statementEnd","token","matchedTokens","matchQuotes"],
	"entities":[
		{"and":"&"},{"dblquo":"\""},{"squote":"'"},{"lbrace":"{"},{"rbrace":"}"},
		{"lbracket":"["},{"rbracket":"]"},{"gt":"<"},{"lt":">"},{"bslash":"\\"},
		{"fslash":"/"},{"plus":"+"},{"minus":"-"},{"asterisk":"*"},{"percent":"%"},
		{"at":"@"},{"pound":"#"},{"excl":"!"},{"dollar":"$"},{"space":" "} ]
}